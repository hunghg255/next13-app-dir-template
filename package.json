{
  "name": "next13-app-dir",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.clientDev next dev",
    "build": "next build",
    "buildDev": "npm run prebuild && cp -f .env.clientDev .env && next build",
    "buildProd": "cp -f .env.clientProd .env && next build",
    "start": "env-cmd -f .env.clientProd next start",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "verify-commit": "verify-commit-msg",
    "prepare": "git-scm-hooks",
    "release": "bumpp -r",
    "gen-icon": "csvgtocss",
    "standalone": "node server.js",
    "gen-color": "esno ./scripts/genColorCss.ts"
  },
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "classnames": "^2.5.1",
    "next": "^14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.4",
    "sonner": "^1.4.41"
  },
  "devDependencies": {
    "@hunghg255/eslint-config-react": "^0.0.26",
    "@next/bundle-analyzer": "^13.5.6",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "20.1.2",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "bumpp": "^9.4.1",
    "cross-env": "^7.0.3",
    "csvgtocss": "^0.0.8",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^13.5.6",
    "esno": "^4.7.0",
    "git-scm-hooks": "^0.0.11",
    "husky": "^8.0.3",
    "sass": "^1.77.4",
    "typescript": "5.0.4",
    "verify-commit-msg": "^0.0.14"
  },
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  }
}
